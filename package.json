  "description": "Custodial Command - January 20, 2026 release with critical bug fixes",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "prebuild": "npm run check",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --external:fs --external:path --external:crypto --external:http --external:https --external:url --external:os --external:stream --external:util --external:events --external:querystring --external:zlib --outdir=dist",
    "postbuild": "echo Skipping bundle patch - using natural Vite bundling",
    "prestart": "npm run validate-env",
    "start": "npm run db:push && cross-env NODE_ENV=production node dist/index.js",
    "railway:start": "npm run db:push && cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:push-safe": "drizzle-kit push || echo 'Database push failed, continuing anyway...'",
    "preview": "vite preview",
    "validate-env": "node -e \"const required = ['DATABASE_URL']; const missing = required.filter(key => !process.env[key]); if (missing.length > 0) { console.error('âŒ Missing required environment variables:', missing.join(', ')); console.error('ðŸ’¡ Please set these variables in your .env file'); process.exit(1); } else { console.log('âœ… Environment variables validated successfully'); }\"",
    "start:clean": "kill -9 $(lsof -ti:5000) 2>/dev/null || true && npm start",
    "test:health": "node comprehensive-test.cjs",
    "test:forms": "node comprehensive-test.cjs",
    "test:debug": "DEBUG=true node comprehensive-test.cjs",
    "test:comprehensive": "node tests/run-all-tests.cjs",
    "test:e2e": "node tests/e2e-user-journey.test.cjs",
    "test:performance": "node tests/performance.test.cjs",
    "test:security": "node tests/security.test.cjs",
    "test:mobile": "node tests/mobile-pwa.test.cjs",
    "analyze:bundle": "node scripts/analyze-bundle.cjs",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "ui:test": "npx --yes playwright test",
    "ui:test:headed": "npx --yes playwright test --headed",
    "ui:test:report": "npx --yes playwright show-report",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "npm run build && npm run start & npx wait-on http://localhost:5000 && lhci autorun && kill $(lsof -ti:5000)",
    "monitor": "node scripts/monitor-health.mjs",
    "monitor:watch": "node scripts/monitor-health.mjs --watch"
  },
